{
  "family": "acceso-alimentario-task",
  "executionRoleArn": "TASK_EXECUTION_ROLE_ARN",
  "taskRoleArn": "TASK_ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "accesoalimentario_front",
      "image": "034781041905.dkr.ecr.sa-east-1.amazonaws.com/acceso-alimentario_releases:abfbee3225cd705656d49b2cfa85aab6a76ce736",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8085,
          "hostPort": 8085
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/acceso-alimentario-front",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "acceso-alimentario-front"
        }
      }
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "1024"
}
