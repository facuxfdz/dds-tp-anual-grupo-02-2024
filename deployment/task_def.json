{
  "family": "acceso-alimentario-task",
  "containerDefinitions": [
    {
      "name": "",
      "image": "",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "Production"
        },
        {
          "name": "DB_HOST",
          "value": "rds-endpoint"
        },
        {
          "name": "DB_PORT",
          "value": "3306"
        },
        {
          "name": "DB_NAME",
          "value": "AccesoAlimentario"
        },
        {
          "name": "DB_USER",
          "value": "production"
        },
        {
          "name": "DB_PASSWORD",
          "value": "YourStrongPassword!123"
        }
      ]
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "1024"
}
