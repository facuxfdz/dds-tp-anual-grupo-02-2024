{
  "family": "acceso-alimentario-task",
  "containerDefinitions": [
    {
      "name": "accesoalimentario",
      "image": "034781041905.dkr.ecr.sa-east-1.amazonaws.com/acceso-alimentario_releases:fa3aa1331a7c60ddbadefa51b3e8f5dcf3bae830",
      "executionRoleArn": "arn:aws:iam::034781041905:role/accesoalimentario-task-exec-role-20241110131511387400000001",
      "taskRoleArn": "arn:aws:iam::034781041905:role/accesoalimentario-task-role-20241110131511391800000003",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "Production"
        },
        {
          "name": "DB_HOST",
          "value": "rds-endpoint"
        },
        {
          "name": "DB_PORT",
          "value": "3306"
        },
        {
          "name": "DB_NAME",
          "value": "AccesoAlimentario"
        },
        {
          "name": "DB_USER",
          "value": "production"
        },
        {
          "name": "DB_PASSWORD",
          "value": "YourStrongPassword!123"
        }
      ]
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "1024"
}
