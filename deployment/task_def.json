{
  "family": "acceso-alimentario-task",
  "executionRoleArn": "TASK_EXECUTION_ROLE_ARN",
  "taskRoleArn": "TASK_ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "accesoalimentario",
      "image": "034781041905.dkr.ecr.sa-east-1.amazonaws.com/acceso-alimentario_releases:abfbee3225cd705656d49b2cfa85aab6a76ce736",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8085,
          "hostPort": 8085
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/acceso-alimentario",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "acceso-alimentario"
        }
      },
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "Production"
        }
      ]
    },
    {
      "name": "accesoalimentario-front",
      "image": "IMAGE_FRONT",
      "essential": true,
      "memory": 256,
      "cpu": 128,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/acceso-alimentario-front",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "acceso-alimentario-front"
        }
      }
    },
    {
      "name": "rabbitmq",
      "image": "rabbitmq:3-management",
      "essential": true,
      "memory": 512,
      "cpu": 128,
      "portMappings": [
        {
          "containerPort": 5672,
          "hostPort": 5672
        },
        {
          "containerPort": 15672,
          "hostPort": 15672
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/rabbitmq",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "rabbitmq"
        }
      }
    },
    {
      "name": "recomendaciones_api",
      "image": "IMAGE_RECOMENDACIONES_API",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/aws/ecs/recomendaciones_api",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "recomendaciones_api"
        }
      }
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "2048",
  "memory": "4096"
}
